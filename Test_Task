        "import math\n",
        "valores = [-math.inf, 89.999, 120, math.inf]\n",
        "categorias = [\"1. <90\" ,\"2. 90–120\", \"3. >120\"]\n",
        "data[\"movie_duration_category\"]= pd.cut(x=data[\"duration2\"],bins=valores,labels =categorias)\n",
